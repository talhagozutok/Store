@model IEnumerable<Order>

@if (Model.Count() > 0)
{
    @foreach (Order order in @Model)
    {
        <div class="my-3">
            <partial name="_OrderDetails" model="@order"/>
        </div>
    }
}
else
{
    <div class="alert alert-info my-3" role="alert">
        <span class="lead">There is no order yet.</span>
    </div>
}
