@model IEnumerable<Category>

<div class="mt-3">
  <h6 class="lead">
    <i class="fa fa-bars text-secondary"></i>
    Categories
  </h6>
</div>

<div class="my-3">
  <ul class="list-group">
        @foreach (var category in @Model)
        {
        <li class="list-group-item">
          <a asp-controller="Product" asp-action="Index" asp-route-categoryId="@category.CategoryId"> @category.CategoryName </a>
        </li>
        }
  </ul>
</div>
